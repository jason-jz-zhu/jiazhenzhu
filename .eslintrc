"extends": "airbnb"
"rules": {
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
  "react/prop-types": [0, { ignore: <ignore>, customValidators: <customValidator> }]
}
"globals": {
  "window": true,
  "document": true
}
"parser": "babel-eslint"
